<!DOCTYPE html>
%= t 'html' => begin
  %= t 'head' => begin
    %= t 'title' => config->{title}
      %= stylesheet 'http://fonts.googleapis.com/css?family=Cabin+Sketch:700|Ubuntu'
      %= stylesheet '/style.admin.css'
      <%= analytics_inc 'UA-23169268-1' %>
  %= end
  %= t 'body' => begin
    %= include 'admin/nav'
    %= t 'div', id => 'container' => begin
        %= t 'div', id => 'page-body' => begin
          <%= content %>
        %= end
    %= end
  %= end
%= end
